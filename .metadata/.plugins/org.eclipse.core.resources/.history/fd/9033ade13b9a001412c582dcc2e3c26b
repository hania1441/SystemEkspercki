package com.sample;

public class Oferta {
	public String panstwo;
	private String miejscowosc;
	private String hotel;
	private double cena;
	public Oferta(String panstwo, String miejscowosc, String hotel, double cena){
		this.setPanstwo(panstwo);
		this.setMiejscowosc(miejscowosc);
		this.setHotel(hotel);
		this.setCena(cena);
	}
	public Oferta() {
		// TODO Auto-generated constructor stub
	}
	public String getPanstwo() {
		return panstwo;
	}
	public void setPanstwo(String panstwo) {
		this.panstwo = panstwo;
	}
	public String getMiejscowosc() {
		return miejscowosc;
	}
	public void setMiejscowosc(String miejscowosc) {
		this.miejscowosc = miejscowosc;
	}
	public String getHotel() {
		return hotel;
	}
	public void setHotel(String hotel) {
		this.hotel = hotel;
	}
	public double getCena() {
		return cena;
	}
	public void setCena(double cena) {
		this.cena = cena;
	}
}
